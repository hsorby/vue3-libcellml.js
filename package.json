{
  "name": "vue3-libcellml.js",
  "version": "0.2.0",
  "description": "Vue 3 plugin for libcellml.js",
  "main": "./dist/vue3-libcellml.js.umd.js",
  "module": "./dist/vue3-libcellml.js.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-libcellml.js.es.js",
      "require": "./dist/vue3-libcellml.js.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Hugh Sorby",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/libcellml/vue3-libcellml.js/issues"
  },
  "homepage": "https://libcellml.org",
  "keywords": [
    "Vue",
    "Vue3",
    "libCellML",
    "CellML"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "lint": "eslint --config eslint.config.mjs",
    "prepublish": "yarn run lint && yarn run build"
  },
  "dependencies": {
    "core-js": "^3.38.1",
    "libcellml.js": "^0.6.2"
  },
  "peerDependencies": {
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@eslint/compat": "^1.2.2",
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/compiler-sfc": "^3.5.12",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.30.0",
    "prettier": "^3.3.3",
    "rollup": "^4.24.3",
    "vite": "^5.4.10",
    "vue": "^3.5.12"
  }
}
