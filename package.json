{
  "name": "vue3-libcellml.js",
  "version": "0.2.0",
  "description": "Vue 3 plugin for libcellml.js",
  "main": "./dist/vue3-libcellml.js.umd.js",
  "module": "./dist/vue3-libcellml.js.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-libcellml.js.es.js",
      "require": "./dist/vue3-libcellml.js.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Hugh Sorby",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/libcellml/vue3-libcellml.js/issues"
  },
  "homepage": "https://libcellml.org",
  "keywords": [
    "Vue",
    "Vue3",
    "libCellML",
    "CellML"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore",
    "prepublish": "yarn run lint && yarn run build"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "libcellml.js": "^0.3.104"
  },
  "peerDependencies": {
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.33",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.4.1",
    "rollup": "^2.72.1",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^2.9.9",
    "vue": "^3.2.36"
  }
}
